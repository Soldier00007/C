#include<stdio.h>
int main()
{
  int a=10;
  do
  {
       printf("a的值是：%d\n",a);
       a=a+1;
  }
  while(a<20);

       return 0;
  }


99乘法表
#include<stdio.h>
void main()
{
 int i,j;
 for(i=1;i<=9;i++){
 for(j=1;j<=9;j++)
 printf("%d*%d=%d",i,j,i*j);
 printf("\n");
 }
}

圆的面积周长
#include<stdio.h>
#define PI 3.14159
#define R 5
int main()
{
 printf("area=%f\n",PI*R*R);
 prinyf("circumference=%f\n",2*PI*R);
 return 0;
}

自然数相关的
1~100自然数之和
#include<stdio.h>
int main()
{
 int i=1,sum=0;
 while(i<100)
 sum+=i；
 printf("%d"\n);
 return 0;
}

寻找质数
#include<stdio.h>
int main()
{
    int i,j;
    for(i=2;j<=100;i++)
   {
     for(j=2;j<=(i/j);j++)
        if(!(i%j))break;
         if(j>(i/j))
        printf("%d是质数\n",i);
    }
    return 0;
}




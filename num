do while 循环
#include<stdio.h>
int main()
{
  int a=10;
  do
  {
       printf("a的值是：%d\n",a);
       a=a+1;
  }
  while(a<20);

       return 0;
  }


99乘法表
#include<stdio.h>
void main()
{
 int i,j;
 for(i=1;i<=9;i++){
 for(j=1;j<=9;j++)
 printf("%d*%d=%d",i,j,i*j);
 printf("\n");
 }
}

圆的面积周长
#include<stdio.h>
#define PI 3.14159
#define R 5
int main()
{
 printf("area=%f\n",PI*R*R);
 prinyf("circumference=%f\n",2*PI*R);
 return 0;
}

自然数相关的
1~100自然数之和
#include<stdio.h>
int main()
{
 int i=1,sum=0;
 while(i<100)
 sum+=i；
 printf("%d"\n);
 return 0;
}

寻找质数
#include<stdio.h>
int main()
{
    int i,j;
    for(i=2;j<=100;i++)
   {
     for(j=2;j<=(i/j);j++)
        if(!(i%j))break;
         if(j>(i/j))
        printf("%d是质数\n",i);
    }
    return 0;
}


比较大小
#include<stdio.h>
int main()
{
  int i=17,j=23,c;
  if(i>j)
  c=i;
  else
  c=j;
  printf("i=%d\n",i);
  printf("j=%d\n",j);
  printf("c=%d",c);
}


每种数据类型所占内存空间的大小
#include<stdio.h>
int main()
{
 printf("Date type    Number of bytes\n");
 printf("----------  --------------- \n");
 printf("char         %d\n",sizeof(char));
 printf("int          %d\n",sizeof(char));
 printf("short int    %d\n",sizeof(short int));
 printf("long int     %d\n",sizeof(long int));
 printf("double       %d\n",sizeof(double))；
 printf("float        %d\n",sizeof(float);
 return 0;
}

判断是否是闰年
#include<stdio.h>
int main()
{
  int i;
  scanf("%d",&i);
 if(((i%4==0)&&(i%100！=0)||(i%400=0)))
  printf("i=%d是闰年"，i);
 else
  printf("i=%d不是闰年"，i);
  return 0;
}

